<h3>Compare your answers to the rest of the <%= PPRN_TITLE %> community.</h3>
<div class="row">

  <div class="col-md-4">
    <span class="help-block">Choose question to view results for.</span>
    <select class="form-control no-focus" id="question-select" data-answer-session-id="<%= @answer_session.id %>" data-json-path="<%= question_frequencies_path %>">
      <% @answer_session.all_reportable_answers.each do |answer| %>
        <option value="<%= answer.question.id %>"><%= markdown answer.question.text %></option>
      <% end %>
    </select>
  </div>
  <div class="col-md-2 col-md-offset-1">

    <div id="freq-table"></div>
  </div>
  <div class="col-md-4 col-md-offset-1">
    <div id="freq-graph"></div>
  </div>

</div>



<%= javascript_include_tag "reports/default" %>
